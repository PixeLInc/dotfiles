{
  "tsserver.npm": "/usr/bin/npm",
  "languageserver": {
    "odin": {
      "command": "ols",
      "filetypes": ["odin"],
      "rootPatterns": ["ols.json"]
    },
    "nim": {
      "command": "nimlsp",
      "filetypes": ["nim"],
      "trace.server": "verbose"
    },
    "ocaml-lsp": {
      "command": "opam",
      "args": ["config", "exec", "--", "ocamllsp"],
      "filetypes": ["ocaml", "reason"]
    },
    "gleam": {
      "command": "gleam",
      "args": ["lsp"],
      "filetypes": ["gleam"],
      "rootPatterns": ["gleam.toml"]
    }
  },
  "coc.preferences.formatOnSaveFiletypes": ["gleam"],
  "rust-analyzer.server.path": "/home/pixel/.cargo/bin/rust-analyzer",
  "elixir.pathToElixirLS": "/mnt/backup_s/coding/git/elixir-ls/release/language_server.sh",
  "zig.path": "/home/pixel/.local/bin/zls"
}
